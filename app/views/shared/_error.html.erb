<%if obj.errors.any?%>
<div class="col-xs-8 col-xs-offset-2">
	<div class="panel panel-danger">
		<div class="panel-heading"><h3 class="panel-title">There are <%=pluralize(obj.errors.full_messages.count,"error")%>. Please Try Again : </h3></div>
		<div class="panel-body">
		<%obj.errors.full_messages.each do |msg|%>
					<li><%=msg%></li>
		<%end%>
		</div>
	</div>
</div>
<%end%>